@Model DB.Order
@{
    ViewBag.Title = "UploadReceiptFile";
    ViewBag.PageName = "Orders";
    ViewBag.SubPageName = "AllOrders";
}

@using (Html.BeginForm("UploadReceiptFile", "Orders", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    
    @Loc.Dic.UploadReceipt<text>:</text>
    <input type="hidden" name="orderId" value="@ViewBag.OrderId" /> 
    <input type="file" name="file"  />
    <input type="submit" value="OK" />
}